{
  "name": "Site Blocker, todo list",
  "version": "0.0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+O"
      },
      "description": "Open the popup for the extension"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_start",
      "js": ["content.js"]
    }
  ],
  "permissions": ["storage", "tabs"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "nemria.png",
      "32": "nemria.png",
      "48": "nemria.png",
      "128": "nemria.png"
    }
  }
}
